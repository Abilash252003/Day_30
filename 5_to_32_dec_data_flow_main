assign Y[7] = (~en & ~A[4] & ~A[3] &  A[2] &  A[1] &  A[0]);
assign Y[8] = (~en & ~A[4] &  A[3] & ~A[2] & ~A[1] & ~A[0]);
assign Y[9] = (~en & ~A[4] &  A[3] & ~A[2] & ~A[1] &  A[0]);
assign Y[10]= (~en & ~A[4] &  A[3] & ~A[2] &  A[1] & ~A[0]);
assign Y[11]= (~en & ~A[4] &  A[3] & ~A[2] &  A[1] &  A[0]);
assign Y[12]= (~en & ~A[4] &  A[3] &  A[2] & ~A[1] & ~A[0]);
assign Y[13]= (~en & ~A[4] &  A[3] &  A[2] & ~A[1] &  A[0]);
assign Y[14]= (~en & ~A[4] &  A[3] &  A[2] &  A[1] & ~A[0]);
assign Y[15]= (~en & ~A[4] &  A[3] &  A[2] &  A[1] &  A[0]);
assign Y[16]= (~en &  A[4] & ~A[3] & ~A[2] & ~A[1] & ~A[0]);
assign Y[17]= (~en &  A[4] & ~A[3] & ~A[2] & ~A[1] &  A[0]);
