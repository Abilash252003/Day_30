///100 Days of RTL///

///Abilash P///

///5:32 decoder in data flow model///

module dec_5_to_32 (A, en, Y);

input  [4:0]A;
input  en;

output [31:0]Y;

assign Y[0] = (~en & ~A[4] & ~A[3] & ~A[2] & ~A[1] & ~A[0]);
assign Y[1] = (~en & ~A[4] & ~A[3] & ~A[2] & ~A[1] &  A[0]);
