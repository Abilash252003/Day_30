assign Y[23]= (~en &  A[4] & ~A[3] &  A[2] &  A[1] &  A[0]);
assign Y[24]= (~en &  A[4] &  A[3] & ~A[2] & ~A[1] & ~A[0]);
assign Y[25]= (~en &  A[4] &  A[3] & ~A[2] & ~A[1] &  A[0]);
assign Y[26]= (~en &  A[4] &  A[3] & ~A[2] &  A[1] & ~A[0]);
assign Y[27]= (~en &  A[4] &  A[3] & ~A[2] &  A[1] &  A[0]);
assign Y[28]= (~en &  A[4] &  A[3] &  A[2] & ~A[1] & ~A[0]);
assign Y[29]= (~en &  A[4] &  A[3] &  A[2] & ~A[1] &  A[0]);
assign Y[30]= (~en &  A[4] &  A[3] &  A[2] &  A[1] & ~A[0]);
assign Y[31]= (~en &  A[4] &  A[3] &  A[2] &  A[1] &  A[0]);


